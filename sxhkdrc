super+Home
  /usr/bin/xrandr --output eDP1 --primary --mode 1600x900 --pos 0x0 --output DP2-2 --off --output DP2 --off
super+End
  /usr/bin/xrandr --output eDP1 --off --output DP2 --off --output DP2-2 --primary --mode 1920x1080 --pos 0x0
super+Insert
  /usr/bin/xrandr --output eDP1 --off --output DP2-2 --off --output DP2 --primary --mode 1920x1080 --pos 0x0
super+Delete
  /usr/bin/lxrandr
XF86AudioLowerVolume
  pulseaudio-ctl down 3
